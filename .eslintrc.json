{
    "extends": "airbnb-base",
    "rules": {
        "no-unused-vars": [
            "error",
            { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
        ],
        "no-var": 0,
        "vars-on-top": 0,
        "no-console": 0,
        "comma-dangle": 0,
        "no-plusplus": 0
    }
}